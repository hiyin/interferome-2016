<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC 
  		"-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
  		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	 <field name="user.uniqueId">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("user.register.ldap.authcate.id.required")}</message>
		</field-validator>
	</field>
	<field name="user.password">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("user.register.ldap.authcate.password.required")}</message>
		</field-validator>
	</field>
	<field name="securityCode">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>${getText("security.code.required")}</message>
		</field-validator>
	</field>
</validators>  		


